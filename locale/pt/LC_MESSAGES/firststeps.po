#: ../../firststeps.rst:80
msgid "unit - string - required"
msgstr ""

#: ../../firststeps.rst:81
msgid "type - string"
msgstr ""

#: ../../firststeps.rst:82
msgid "time - string/js_dateformat"
msgstr ""

#: ../../firststeps.rst:83
msgid "location - string/geojson"
